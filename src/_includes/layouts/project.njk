---
layout: layouts/layout.njk
pagination:
  data: projects
  size: 1
  alias: project
permalink: "/projects/{{ project.name | slugify }}/"
---
<article>
    <h1>{{ title }}</h1>
    <p>{{ subtitle }}</p>
    <img src="{{ image }}" alt="{{ title }}">
    <p>Technologies Used: {{ tech | join: ', ' }}</p>
    <a target="_blank" href="{{ liveSite }}">Visit Live Site</a>
</article>
